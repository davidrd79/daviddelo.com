{
  "extends": "airbnb",
  "plugins": [
    "mocha",
    "chai-expect"
  ],
  "env": {
    "mocha": true
  },
  "rules": {
    "chai-expect/missing-assertion": 2,
    "chai-expect/terminating-properties": 1,
    "chai-expect/no-inner-compare": 1,
    "comma-dangle": 0,
    "mocha/no-global-tests": 1,
    "no-unused-expressions": 0
  }
}
